        <div class="dadosConta">
            <img class="fotoPerfilLogada" src="data:image/png;base64,{{fotoPerfilBase64}}" alt="fot perfil">
            <p class="nomeContaLogada" name="nomeContaLogada">{{usuario.nome}}</p>
            <label for="" name="bioContaLogada" id="bioContaLogada">{{usuario.bio}}</label>

        </div>
