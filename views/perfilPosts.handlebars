    <div>
        {{#each postsConta}}

        <div class="post">
            <a href="/perfilPost/{{idpost}}" class="nomeperfil"><img class="fotoPerfil"
                    src="data:image/png;base64,{{foto_perfil}}" alt="fot perfil">
                <p>{{username}}</p>
            </a>
            <img class="imagemPost" src="data:image/png;base64,{{imagem}}" alt="conteudo post">
            <div class="div-actions">
                <form action="/enviarCurtida/{{idpost}}" class="likes" method="post">
                    <button><i
                            class='bx bx-heart btn-actions like-button {{#if isLiked}}liked bxs-heart{{/if}}'></i></button>
                </form>
                <span>{{likes}}</span>
                <a href="/addComment/{{idpost}}"><i class='bx bx-comment comment'></i></a>
                <span>{{comments}}</span>
            </div>
            <div class="content-inline">
                <h2>{{username}}</h2>
                <p>{{legenda}}</p>
            </div>
        </div>
        {{/each}}
