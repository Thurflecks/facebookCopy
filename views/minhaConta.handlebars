                    </svg></a>
                <a href="/notificacoes"><i class='bx bx-bell'></i></a>
                <a href="/minhaConta"><img class="minhaContaNav minhacontaAtivate"
                        src="data:image/png;base64,{{fotoPerfil}}" alt="fot perfil"></a>
            </div>
        </nav>
        <div class="dadosConta">
            <form class="logout" action="/logout" method="POST">
                <button type="submit">logout</button>
            </form>
            <img class="fotoPerfilLogada" src="data:image/png;base64,{{fotoPerfil}}" alt="fot perfil">
            <p class="nomeContaLogada" name="nomeContaLogada">{{usuario.nome}}</p>
            <label for="" name="bioContaLogada" id="bioContaLogada">{{usuario.bio}}</label>
            <a href="/minhaConta/editarPerfil" class="botaoEditarConta">Editar Perfil</a>

        </div>
    </div>
    <div>
        {{#each postsConta}}
        <div class="post">
            <div class="post-header">
                <a href="#" class="nomeperfil">
                    <img class="fotoPerfil" src="data:image/png;base64,{{foto_perfil}}" alt="foto perfil">
                    <p>{{username}}</p>
                </a>
                <div class="post-actions">
                    <a href="/editPost/{{idpost}}" class="edit-btn">Editar</a>
                    <form action="/post/delete/{{idpost}}" method="POST"
                        onsubmit="return confirm('Tem certeza que deseja deletar este post?');">
                        <button type="submit" class="delete-btn">Deletar</button>
                    </form>
                </div>
            </div>
            <img class="imagemPost" src="data:image/png;base64,{{imagem}}" alt="conteudo post">
            <div class="div-actions">
                <form action="/enviarCurtida/{{idpost}}" class="likes" method="post">
                    <button><i
                            class='bx bx-heart btn-actions like-button {{#if isLiked}}liked bxs-heart{{/if}}'></i></button>
                </form>
                <span>{{likes}}</span>
                <a href="/addComment/{{idpost}}"><i class='bx bx-comment comment'></i></a>
                <span>{{comments}}</span>
            </div>
            <div class="content-inline">
                <h2>{{username}}</h2>
                <p>{{legenda}}</p>
            </div>
        </div>
        {{/each}}

    </div>
    <div>
        <div class="div-fim">
            <p>by Thurflecks</p>
            <p>Todos os direitos reservados para FACEBOOK</p>
        </div>
    </div>
</div>